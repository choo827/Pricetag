{
  "name": "@pricetag/api-server",
  "description": "API service for pricetag",
  "version": "0.15.9",
  "scripts": {
    "dev": "serverless offline",
    "dev:prod": "serverless offline --stage v1",
    "build": "serverless package",
    "deploy:prod": "read -p \"[PRODUCTION WARNING]\nAre you sure deploying to PROD?\nEnter stage again. \" -r\necho\nif [[ $REPLY =~ v1$ ]]\nthen serverless prune -n 10 --stage v1 && serverless create_domain --stage v1 && serverless deploy --stage v1\nfi",
    "deploy:stg": "serverless prune -n 10 --stage stg && serverless create_domain --stage stg && serverless deploy --stage stg",
    "run-ts:prod": "STAGE=v1 ts-node -r tsconfig-paths/register",
    "run-ts:stg": "STAGE=stg ts-node -r tsconfig-paths/register"
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.943.0",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.78",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.1",
    "dotenv-webpack": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "serverless": "^2.51.2",
    "serverless-domain-manager": "^5.1.0",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^7.0.0",
    "serverless-offline-scheduler": "^0.5.0",
    "serverless-plugin-warmup": "^5.3.1",
    "serverless-prune-plugin": "^1.5.1",
    "serverless-webpack": "^5.5.1",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-node-externals": "^3.0.0"
  }
}